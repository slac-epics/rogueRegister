# liDevRogue.template
record( longin, "$(P)$(R)$(NAME)" )
{
	field( DESC, "$(DESC=?)" )
	field( DTYP, "RogueLI" )
	field( INP,  "@B$(B) L$(L) $(PATH)" )
	field( PINI, "$(PINI=YES)" )
	field( SCAN, "$(SCAN=)" )
	field( DISV, "$(DISV=1)" )
	field( SDIS, "$(SDIS=)" )
	info( autosaveFields, "" )
}
alias( "$(P)$(R)$(NAME)", "$(P)$(R)$(PYPV)" )
