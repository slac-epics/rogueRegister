# boolDevRogue.template
record( bi, "$(P)$(R)$(NAME)_RBV" )
{
	field( DESC, "$(DESC=?)" )
	field( DTYP, "RogueBI" )
	field( INP,  "@B$(B) L$(L) $(PATH)" )
	field( SCAN, "$(SCAN=)" )
	field( ZNAM, "$(ZNAM=Disabled)" )
	field( ONAM, "$(ONAM=Enabled)" )
	field( DISV, "$(DISV=1)" )
	field( SDIS, "$(SDIS=)" )
	info( autosaveFields, "" )
}
alias( "$(P)$(R)$(NAME)_RBV", "$(P)$(R)$(PYPV)_RBV" )
include "boDevRogue.template"
record( bo, "$(P)$(R)$(NAME)" )
{
	field( FLNK, "$(P)$(R)$(NAME)_RBV" )
}
